          <style xsi:type="style:SquareDescription" labelExpression="service:getSystemUsagesText()" labelPosition="node" resizeKind="NSEW" width="18" height="8">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="OperationNode" domainClass="prologmodel::Operation">
          <borderedNodeMappings name="Properties" semanticCandidatesExpression="feature:eAllContents" domainClass="prologmodel::PropertyDefinition">
            <style xsi:type="style:SquareDescription" borderSizeComputationExpression="1" labelSize="7" labelExpression="service:getPropertyDefinitionText()" sizeComputationExpression="14" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <forbiddenSides>WEST</forbiddenSides>
              <forbiddenSides>SOUTH</forbiddenSides>
              <forbiddenSides>NORTH</forbiddenSides>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            </style>
          </borderedNodeMappings>
          <borderedNodeMappings name="returnValue" semanticCandidatesExpression="feature:returnValues" domainClass="prologmodel::Variable">
            <style xsi:type="style:SquareDescription" borderSizeComputationExpression="1" labelExpression="service:getReturnValueText()" labelPosition="node" resizeKind="NSEW" width="10" height="10">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <forbiddenSides>NORTH</forbiddenSides>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="1" labelExpression="service:getText()" labelPosition="node" resizeKind="NSEW" width="18" height="8">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="calingRelation" sourceMapping="//@ownedViewpoints[name='PrologModel']/@ownedRepresentations[name='PrologModel']/@defaultLayer/@nodeMappings[name='OperationNode']" targetMapping="//@ownedViewpoints[name='PrologModel']/@ownedRepresentations[name='PrologModel']/@defaultLayer/@nodeMappings[name='OperationNode']" targetFinderExpression="feature:callee" sourceFinderExpression="feature:caller" domainClass="prologmodel::OperationCall" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <beginLabelStyleDescription labelExpression="service:getOreCallAssignment()">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </beginLabelStyleDescription>
            <centerLabelStyleDescription labelExpression="service:operationNumber()">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="SystemUsageToOperationCall" sourceMapping="//@ownedViewpoints[name='PrologModel']/@ownedRepresentations[name='PrologModel']/@defaultLayer/@nodeMappings[name='Usages']" targetMapping="//@ownedViewpoints[name='PrologModel']/@ownedRepresentations[name='PrologModel']/@defaultLayer/@nodeMappings[name='OperationNode']" targetFinderExpression="feature:callee" sourceFinderExpression="feature:caller" domainClass="prologmodel::OperationCall" useDomainElement="true">
