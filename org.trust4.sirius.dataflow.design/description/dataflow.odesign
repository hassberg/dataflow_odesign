          </style>
        </nodeMappings>
        <edgeMappings name="returnValueAssign" sourceMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@additionalLayers[name='Helpfull%2C%20false%20']/@nodeMappings[name='Return%20Value']" targetMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@defaultLayer/@edgeMappings[name='callingrelation']" targetFinderExpression="service:getReturnValue()">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="helpfull1" sourceMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@additionalLayers[name='Helpfull%2C%20false%20']/@nodeMappings[name='Return%20Value']" targetFinderExpression="service:getReturnValueToCallee()">
          <style lineStyle="dot" targetArrow="NoDecoration" sizeComputationExpression="0">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="helpfull2" sourceMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@additionalLayers[name='Helpfull%2C%20false%20']/@nodeMappings[name='Return%20Value']" targetFinderExpression="service:getReturnValueToCaller()">
          <style lineStyle="dot" targetArrow="NoDecoration" sizeComputationExpression="0">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="ReturnValueNamed" targetFinderExpression="feature:caller" sourceFinderExpression="feature:callee" domainClass="systemmodel::OperationCall" useDomainElement="true">
