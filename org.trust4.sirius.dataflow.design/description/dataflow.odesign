              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings sourceMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@defaultLayer/@nodeMappings[name='Usages']" targetMapping="//@ownedViewpoints[name='SystemModel']/@ownedRepresentations[name='Overview']/@defaultLayer/@nodeMappings[name='OperationNode']" targetFinderExpression="feature:callee" sourceFinderExpression="feature:caller" domainClass="systemmodel::OperationCall" useDomainElement="true">
